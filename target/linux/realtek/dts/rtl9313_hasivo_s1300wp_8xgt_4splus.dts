/dts-v1/;

#include "rtl931x.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/leds/common.h>

/ {
	compatible = "hasivo,s1300wp-8xgt-4s+", "realtek,rtl931x-soc";
	model = "Hasivo S1300WP-8XGT-4S+";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x10000000>, /* 256 MiB lowmem */
		      <0x20000000 0x10000000>; /* 256 MiB highmem */
	};

	chosen {
		stdout-path = "serial0:38400n8";
	};
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <1000000>;
		
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			/* stock is LOADER */
			partition@0 {
				label = "u-boot";
				reg = <0x00000000 0x000e0000>;
				read-only;
			};

			/* stock is BDINFO */
			partition@e0000 {
				label = "u-boot-env";
				reg = <0x000e0000 0x0001000>;
			};

			/* stock is SYSINFO */
			partition@f0000 {
				label = "u-boot-env2";
				reg = <0x000f0000 0x00010000>;
			};

			/* stock is JFFS2_CFG */
			partition@100000 {
				label = "jffs";
				reg = <0x00100000 0x00100000>;
			};

			/* stock is JFFS2_LOG */
			partition@200000 {
				label = "jffs2";
				reg = <0x00200000 0x00100000>;
			};

			/* stock is RUNTIME */
			partition@300000 {
				compatible = "openwrt,uimage", "denx,uimage";
				label = "firmware";
				reg = <0x00300000 0x00700000>;
			};

			/* stock is RUNTIME2 */
			partition@a00000 {
				label = "oeminfo";
				reg = <0x00a00000 0x01600000>;
				read-only;
			};
		};
	};
};

&ethernet0 {
	mdio: mdio-bus {
		compatible = "realtek,rtl838x-mdio";
		regmap = <&ethernet0>;
		#address-cells = <1>;
		#size-cells = <0>;

		phy0: ethernet-phy@0 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <0 0>;
			reg = <0>;
			sds = <2>;
		};

		phy8: ethernet-phy@8 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <0 1>;
			reg = <8>;
			sds = <3>;
		};

		phy16: ethernet-phy@16 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <0 2>;
			reg = <16>;
			sds = <4>;
		};

		phy24: ethernet-phy@24 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <0 3>;
			reg = <24>;
			sds = <5>;
		};

		phy32: ethernet-phy@32 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <3 16>;
			reg = <32>;
			sds = <6>;
		};

		phy40: ethernet-phy@40 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <3 17>;
			reg = <40>;
			sds = <7>;
		};

		phy48: ethernet-phy@48 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <3 18>;
			reg = <48>;
			sds = <8>;
		};

		phy50: ethernet-phy@50 {
			compatible = "ethernet-phy-ieee802.3-c45";
			rtl9300,smi-address = <3 19>;
			reg = <50>;
			sds = <9>;
		};
	};
};

&switch0 {
	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			label = "lan1";
			phy-mode = "usxgmii";
			phy-handle = <&phy0>;
			led-set = <0>;
		};

		port@8 {
			reg = <8>;
			label = "lan2";
			phy-mode = "usxgmii";
			phy-handle = <&phy8>;
			led-set = <0>;
		};

		port@16 {
			reg = <16>;
			label = "lan3";
			phy-mode = "usxgmii";
			phy-handle = <&phy16>;
			led-set = <0>;
		};

		port@24 {
			reg = <24>;
			label = "lan4";
			phy-mode = "usxgmii";
			phy-handle = <&phy24>;
			led-set = <0>;
		};

		port@32 {
			reg = <32>;
			label = "lan5";
			phy-mode = "usxgmii";
			phy-handle = <&phy32>;
			led-set = <0>;
		};

		port@40 {
			reg = <40>;
			label = "lan6";
			phy-mode = "usxgmii";
			phy-handle = <&phy40>;
			led-set = <0>;
		};

		port@48 {
			reg = <48>;
			label = "lan7";
			phy-mode = "usxgmii";
			phy-handle = <&phy48>;
			led-set = <0>;
		};

		port@50 {
			reg = <50>;
			label = "lan8";
			phy-mode = "usxgmii";
			phy-handle = <&phy50>;
			led-set = <0>;
		};

		port@56 {
			ethernet = <&ethernet0>;
			reg = <56>;
			phy-mode = "internal";

			fixed-link {
				speed = <10000>;
				full-duplex;
			};
		};
	};
};
